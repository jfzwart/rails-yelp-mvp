<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>

<%= form_for([@restaurant, @review]) do |f| %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.submit "Submit review", class: "btn btn-primary"%>

<% end %>
